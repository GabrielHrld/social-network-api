{
  "version": 2,
  "builds": [
      {
          "src": "api/index.js",
          "use": "@vercel/node"
      },
      {
          "src": "post_service/index-post.js",
          "use": "@vercel/node"
      }
  ],
  "routes": [
      {
          "src": "/api/auth(.*)",
          "dest": "/api/index.js"
      },
      {
          "src": "/api/user(.*)",
          "dest": "/api/index.js"
      },
      {
          "src": "/api/post(.*)",
          "dest": "/post_service/index.js"
      }
  ]
}